Function xValid( aMtr1 , aLabelx )
	Local lOk := .t.

	LOCAL n2
	Local vValor
	Local cType

	FOR n2 := 1 TO Len( aMtr1 )

		If aMtr1[ n2 ][ 2 ]

			cType  := aMtr1[ n2 ][ 3 ]
			vValor := _GetValue ( aMtr1[ n2 ][ 1 ]  , '_InputWindow' )

			If (cType == 'I') .and. (lOk)

				If (vValor = 0)

					xStop( aMtr1[ n2 ][ 1 ] , aLabelx[n2] )
					lOk := .f.

				End If

			End If

			If (cType == 'C') .and. (lOk)

				If Empty(alltrim(vValor  , aLabelx[n2] ))
					xStop( aMtr1[ n2 ][ 1 ] , aLabelx[n2] )
					lOk := .f.
				End If

			End If

			If (cType == 'D') .and. (lOk)

				If (vValor == Ctod('  /  /    '))
					xStop( aMtr1[ n2 ][ 1 ] , aLabelx[n2] )
					lOk := .f.
				End If

			End If

		End If
	Next

Return lOk

